<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ephraimhammer.jct.customercarrental">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".control.activity.MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".control.other.BranchsModels"
            android:label="@string/title_activity_branchs_models"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".control.CarListActivity" />

        <service
            android:name=".control.reservedCarUpdateService"
            android:exported="false"></service>
        <activity android:name=".control.activity.CarListActivity" />
        <activity android:name=".control.activity.branchSimpleListActivity"></activity>
    </application>

</manifest>