<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ephraimhammer.jct.customercarrental">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".control.MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".control.CarsBanches"
            android:label="@string/title_activity_cars_banches"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".control.BranchsModels"
            android:label="@string/title_activity_branchs_models"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>